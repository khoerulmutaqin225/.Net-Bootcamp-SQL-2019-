create table hr.DEPARTMENTS(
	department_id int identity(1,1)	constraint department_id_pk primary key,
	department_name	varchar(30) default null,
	location_id int,
	manager_id int,
	constraint location_id_fk foreign key(location_id) references hr.LOCATIONS(location_id)
---constraint manager_id_fk foreign key(manager_id) references hr.EMPLOYEES(employee_id)
);
