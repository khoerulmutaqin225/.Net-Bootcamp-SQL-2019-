----- DAY 1 SEC 1 (15/8/2022)-----
--- 1. Membuat table ---
--- 2. Penjelasan tipe data (char,int,string,varchar,boolen)
--- create table biasa (Membuat table) selanjutnya teks di blok dan di execute --- 
create table regions(
	regionid int identity(1,1) primary key,
	region_name varchar(25) default null
);

--- 3. Memnghapus table ---
drop table regions

--- 4. Menampilkan table ---
select *from regions



--- 5. create table dengan constraint ---
create table regions(
	region_id int identity(1,1),
	region_name varchar(25) default null,
	constraint region_id_pk primary key(region_id)
);

--- 6. Insert data to table (Ditambah identity karena region id di susun atas identity) ---
set identity_insert regions on;
insert into regions (region_id,region_name) values(1,'Europe');
set identity_insert regions off;
select *from regions


--- 7. Insert data to table (without hardcore)
insert into regions (region_name) values('Europe');
select *from regions

--- 8. Menampilkan data dengan filter
select *from regions where region_id=3

--- 9. Menghapus table setelah filter
delete from regions where region_id=3
insert into regions (region_name) values('Europe');

--- 10. menghapus table 
drop table regions;


--- 11. create table countries
create table countries(
 country_id char(2) constraint country_id_pk primary key,
 country_name varchar(25),
 region_id int,
 constraint region_id_fk foreign key(region_id) references regions(region_id)
);

--- 12. Insert data ke countries
insert into countries(country_id,country_name,region_id) values('IN','INDONESIA',4)

--- 13. Menampilkan data countries
select from countries 

--- 14. Menghapus data countries



