create table hr.JOB_HISTORY (
	employee_id int,
	start_date datetime,
	end_date datetime,
	job_id varchar(10),
	department_id int,
	constraint JHS_employee_id_pk primary key(employee_id,start_date), 
	constraint JHS_job_id_fk foreign key(job_id) references hr.JOBS(job_id),
	constraint JHS_department_id_fk foreign key(department_id) references hr.DEPARTMENTS(department_id)
);


drop table hr.JOB_HISTORY

